{
    "name": "Transposer",
    "description": "Transposer is a Chrome Extension for quickly transposing chords on websites.",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
      {
          "matches": [
              "<all_urls>"
          ],
          "js": [
              "backend/logic/transpose.js",
              "backend/logic/replace.js",
              "backend/content.js"
          ]
      }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "action": {
        "default_popup": "frontend/popup.html",
        "default_title": "Transpose chords on this page"
    }
  }
